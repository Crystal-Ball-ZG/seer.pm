#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/var/www/seer.pm/node_modules/.pnpm/browserslist@4.20.2/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules/@babel/helper-compilation-targets/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules/@babel/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/var/www/seer.pm/node_modules/.pnpm/browserslist@4.20.2/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules/@babel/helper-compilation-targets/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules/@babel/node_modules:/var/www/seer.pm/node_modules/.pnpm/@babel+helper-compilation-targets@7.17.7_@babel+core@7.16.12/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../browserslist@4.20.2/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../browserslist@4.20.2/node_modules/browserslist/cli.js" "$@"
fi
