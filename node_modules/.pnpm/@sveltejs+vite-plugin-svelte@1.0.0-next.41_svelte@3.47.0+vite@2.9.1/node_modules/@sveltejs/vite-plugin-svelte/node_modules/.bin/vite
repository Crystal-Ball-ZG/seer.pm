#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/vite-plugin-svelte/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules/@sveltejs/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+vite-plugin-svelte@1.0.0-next.41_svelte@3.47.0+vite@2.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.9.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.9.1/node_modules/vite/bin/vite.js" "$@"
fi
