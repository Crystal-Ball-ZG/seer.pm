#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules/esbuild/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules/esbuild/node_modules:/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/esbuild/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/esbuild/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/@sveltejs/adapter-netlify/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/@sveltejs/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules/esbuild/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules/esbuild/node_modules:/var/www/seer.pm/node_modules/.pnpm/esbuild@0.14.36/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/esbuild/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/esbuild/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/@sveltejs/adapter-netlify/node_modules:/var/www/seer.pm/node_modules/.pnpm/@sveltejs+adapter-netlify@1.0.0-next.51/node_modules/@sveltejs/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../esbuild/bin/esbuild" "$@"
fi
