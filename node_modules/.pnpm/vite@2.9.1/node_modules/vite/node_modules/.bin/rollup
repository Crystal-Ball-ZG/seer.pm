#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/dist/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/dist/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/node_modules:/var/www/seer.pm/node_modules/.pnpm/rollup@2.70.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/dist/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/dist/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/rollup/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../rollup/dist/bin/rollup" "$@"
fi
