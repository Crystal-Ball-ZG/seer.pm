#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules/windicss/cli/node_modules:/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules/windicss/node_modules:/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/svelte-windicss-preprocess@4.2.6/node_modules/svelte-windicss-preprocess/node_modules:/var/www/seer.pm/node_modules/.pnpm/svelte-windicss-preprocess@4.2.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules/windicss/cli/node_modules:/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules/windicss/node_modules:/var/www/seer.pm/node_modules/.pnpm/windicss@3.2.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/svelte-windicss-preprocess@4.2.6/node_modules/svelte-windicss-preprocess/node_modules:/var/www/seer.pm/node_modules/.pnpm/svelte-windicss-preprocess@4.2.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../windicss@3.2.1/node_modules/windicss/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../windicss@3.2.1/node_modules/windicss/cli/index.js" "$@"
fi
