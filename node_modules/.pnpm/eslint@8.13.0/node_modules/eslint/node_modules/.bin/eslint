#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules/eslint/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules/eslint/node_modules:/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules/eslint/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules/eslint/node_modules:/var/www/seer.pm/node_modules/.pnpm/eslint@8.13.0/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/eslint.js" "$@"
else
  exec node  "$basedir/../../bin/eslint.js" "$@"
fi
