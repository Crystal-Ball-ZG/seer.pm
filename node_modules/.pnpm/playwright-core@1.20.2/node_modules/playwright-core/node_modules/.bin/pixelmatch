#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules/pixelmatch/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules/pixelmatch/node_modules:/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/pixelmatch/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/pixelmatch/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/playwright-core/node_modules"
else
  export NODE_PATH="$NODE_PATH:/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules/pixelmatch/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules/pixelmatch/node_modules:/var/www/seer.pm/node_modules/.pnpm/pixelmatch@5.2.1/node_modules:/var/www/seer.pm/node_modules/.pnpm/node_modules:/var/www/seer.pm/node_modules:/var/www/node_modules:/var/node_modules:/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/pixelmatch/bin/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/pixelmatch/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules:/var/www/seer.pm/node_modules/.pnpm/playwright-core@1.20.2/node_modules/playwright-core/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../pixelmatch/bin/pixelmatch" "$@"
else
  exec node  "$basedir/../../../pixelmatch/bin/pixelmatch" "$@"
fi
