{
	"name": "farseer",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"start": "git pull && svelte-kit build && sudo systemctl restart seer",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@core": "link:unocss/@core",
		"@iconify/json": "^2.1.31",
		"@playwright/test": "^1.21.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.73",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"@unocss/core": "^0.31.12",
		"@unocss/preset-icons": "^0.31.12",
		"@unocss/preset-uno": "^0.31.12",
		"@unocss/preset-web-fonts": "^0.31.12",
		"@unocss/reset": "^0.31.12",
		"@unocss/vite": "^0.31.12",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.4.0",
		"typescript": "~4.6.3",
		"vite": "^2.9.5"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.7",
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.5.0"
	}
}
